public class TestStudent {
	public static void main (String[] args){
		Student s1, s2;
		
		s1 = new Student();
		s1.setName("Bill Whittle");
		s1.setScore(1, 80);
		s1.setScore(2, 84);
		s1.setScore(3, 88);
		System.out.println("\nhere is student s1\n" + s1);
		
		s2 = s1;
		s2.setName("Ayn Rand");
		System.out.println("\nName of s1 is now: " + s1.getName());
	}
}
